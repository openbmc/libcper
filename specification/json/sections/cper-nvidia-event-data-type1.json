{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "description": "Type 1 context data (dataFormatType=1) - 64-bit key-value pairs",
  "additionalProperties": false,
  "required": ["keyValArray64"],
  "properties": {
    "keyValArray64": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["key64", "val64"],
        "additionalProperties": false,
        "properties": {
          "key64": {
            "type": "string",
            "pattern": "^0x[0-9a-fA-F]{16}$"
          },
          "val64": {
            "type": "string",
            "pattern": "^0x[0-9a-fA-F]{16}$"
          }
        }
      }
    },
    "data": {
      "type": "string",
      "pattern": "^[A-Za-z0-9+/]*={0,2}$"
    }
  }
}
