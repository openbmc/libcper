{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "description": "GPU legacy XID context data (GPU dataFormatType=0x8001)",
  "additionalProperties": false,
  "required": ["xidCode", "message"],
  "properties": {
    "xidCode": {
      "type": "integer",
      "description": "Legacy XID error code",
      "minimum": 0
    },
    "message": {
      "type": "string",
      "description": "NUL-terminated ASCII event message"
    }
  }
}
