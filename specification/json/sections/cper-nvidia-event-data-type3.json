{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "cper-json-nvidia-event-data-type3",
    "type": "object",
    "description": "Type 3 context data (dataFormatType=3) - 64-bit value array",
    "additionalProperties": false,
    "required": ["valArray64"],
    "properties": {
        "valArray64": {
            "type": "array",
            "items": {
                "type": "object",
                "required": ["val64"],
                "additionalProperties": false,
                "properties": {
                    "val64": { "type": "integer", "minimum": 0 }
                }
            }
        },
        "data": { "type": "string", "pattern": "^[A-Za-z0-9+/]*={0,2}$" }
    }
}
