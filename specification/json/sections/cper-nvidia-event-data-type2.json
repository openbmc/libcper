{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "cper-json-nvidia-event-data-type2",
    "type": "object",
    "description": "Type 2 context data (dataFormatType=2) - 32-bit key-value pairs",
    "additionalProperties": false,
    "required": ["keyValArray32"],
    "properties": {
        "keyValArray32": {
            "type": "array",
            "items": {
                "type": "object",
                "required": ["key32", "val32"],
                "additionalProperties": false,
                "properties": {
                    "key32": { "type": "integer", "minimum": 0 },
                    "val32": { "type": "integer", "minimum": 0 }
                }
            }
        },
        "data": { "type": "string", "pattern": "^[A-Za-z0-9+/]*={0,2}$" }
    }
}
